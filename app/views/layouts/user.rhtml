                               <div id="linksmenu" align="center">
                               <%= link_to 'Profile',:controller=> 'spec',:action=> 'index',:class=> 'a'%>
                               <%= link_to 'Messages',:controller=> 'messages',:action=> 'index',:class=> 'a'%>
                               <%= link_to 'Photos',:controller=> 'albums',:action=> 'index',:class=> 'a'%>
                               <%= link_to 'Videos',:controller=> 'videos',:action=> 'index',:class=> 'a'%>
                               <%= link_to 'Events',:controller=> 'events',:action=> 'index',:class=> 'a'%>
                               <a href="/blogs/<%= if @blog = Blog.find(:first, :conditions=>{:user_id=> current_user.id})
                                                                    @blog.id end -%>/posts">Blogs</a>
                               <%= link_to 'Audios',:controller=> 'sounds',:action=> 'index',:class=> 'a'%>
                                </div>                          
                             </div>                               
                               <span>
                                <a href="/emailer" >Invite Friends</a>
                              </span>
                                </div>
                            </div>                            
                        </div>
                    </div>
                    <div class="art-content">
