<div style="margin-left:20px;" align="left">
   <span class="art-button-wrapper">
   <span class="l"> </span>
   <span class="r"> </span>
  <%= button_to "Add",:action=>'new',:class=> 'art-button'  %>
   </span>

<%= link_to image_tag('add_button.png'),:action=>'new'  %>
<p><h2>Videos</h2></p><br />
<%= will_paginate @videos %>
	<div class = "video_container">
		<% for video in @videos do %>

          <blockquote>   
	  <a href = "<%= video_url(video) %>" class = "video_thumb">
		<%= video.thumbnail ? image_tag(video.thumbnail.public_filename) : "No Thumbnail Available" %>
	<p><%= video.title %></p>
          </a>

          </blockquote>
		<% end %>		
	</div>
<div class = "clear-right"></div>

<div align="center">	<%= will_paginate @videos, :class => "pagination page-wide" %>
</div>
	<br />

</div>