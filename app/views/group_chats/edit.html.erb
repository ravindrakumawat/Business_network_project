<h1>Editing group_chat</h1>

<% form_for(@group_chat) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :chat_field %><br />
    <%= f.text_area :chat_field %>
  </p>
  <p>
    <%= f.label :user_name %><br />
    <%= f.text_field :user_name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @group_chat %> |
<%= link_to 'Back', group_chats_path %>